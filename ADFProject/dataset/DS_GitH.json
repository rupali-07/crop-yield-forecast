{
	"name": "DS_GitH",
	"properties": {
		"description": "Dynamic dataset to get data from git\n",
		"parameters": {
			"file_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": {
					"value": "rupali-07/rupali-07/refs/heads/main/@{dataset().file_name}",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}