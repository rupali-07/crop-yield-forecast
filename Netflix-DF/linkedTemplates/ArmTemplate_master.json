{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Netflix-DF"
		},
		"AzureDataLakeStorage_forcropyeild_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureDataLakeStorage_forcropyeild'"
		},
		"LS_Blob_Agri_Data_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LS_Blob_Agri_Data'"
		},
		"AzureMySql427_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/658e09a5-4f3e-4967-b3a6-646522655db5/resourceGroups/Netflix-DF/providers/Microsoft.DBforMySQL/flexibleServers/flexibleserver"
		},
		"AzureMySql427_properties_groupId": {
			"type": "string",
			"defaultValue": "mysqlServer"
		},
		"AzureMySql427_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"flexibleserver.mysql.database.azure.com"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Netflix-DF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_forcropyeild_sasUri": {
						"value": "[parameters('AzureDataLakeStorage_forcropyeild_sasUri')]"
					},
					"LS_Blob_Agri_Data_sasUri": {
						"value": "[parameters('LS_Blob_Agri_Data_sasUri')]"
					},
					"AzureMySql427_properties_privateLinkResourceId": {
						"value": "[parameters('AzureMySql427_properties_privateLinkResourceId')]"
					},
					"AzureMySql427_properties_groupId": {
						"value": "[parameters('AzureMySql427_properties_groupId')]"
					},
					"AzureMySql427_properties_fqdns": {
						"value": "[parameters('AzureMySql427_properties_fqdns')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}